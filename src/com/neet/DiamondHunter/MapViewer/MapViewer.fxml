<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.neet.DiamondHunter.MapViewer.MapViewerController">
    <Canvas fx:id="canvas" height="640.0" onMouseClicked="#setLocation" onMouseEntered="#validation" width="640.0"
            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    <Button fx:id="btnAxe" layoutX="25.0" layoutY="700.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="75.0"
            text="Axe"/>
    <Button fx:id="btnBoat" layoutX="150.0" layoutY="700.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="75.0"
            text="Boat"/>
    <Button layoutX="275.0" layoutY="700.0" mnemonicParsing="false" onAction="#resetToDefaultCoordinates"
            prefHeight="50.0" prefWidth="75.0" text="Default"/>
    <Button layoutX="400.0" layoutY="700.0" mnemonicParsing="false" onAction="#saveNewCoordinates" prefHeight="50.0"
            prefWidth="75.0" text="Save"/>
    <Button layoutX="525.0" layoutY="700.0" mnemonicParsing="false" onAction="#showInformation" prefHeight="50.0"
            prefWidth="75.0" text="Info"/>
</AnchorPane>
