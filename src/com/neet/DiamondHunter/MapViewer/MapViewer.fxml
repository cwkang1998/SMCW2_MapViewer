<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="760.0" prefWidth="640.0" style="-fx-background-color: #D4EDD2;"
            xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.neet.DiamondHunter.MapViewer.MapViewerController">

    <Canvas fx:id="canvas" height="640.0" onMouseClicked="#setLocation" onMouseEntered="#validation" width="640.0"
            AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>


    <Button fx:id="btnAxe" layoutX="5.0" layoutY="645.0" mnemonicParsing="false" prefHeight="110.0" prefWidth="110.0"
            style="-fx-background-color:#a8dca3;" text="Axe"/>
    <Button fx:id="btnBoat" layoutX="135.0" layoutY="645.0" mnemonicParsing="false" prefHeight="110.0" prefWidth="110.0"
            style="-fx-background-color:#a8dca3;" text="Boat"/>
    <Button layoutX="265.0" layoutY="645.0" mnemonicParsing="false" onAction="#resetToDefaultCoordinates"
            prefHeight="110.0" prefWidth="110.0" style="-fx-background-color:#a8dca3;" text="Default"/>
    <Button layoutX="396.0" layoutY="645.0" mnemonicParsing="false" onAction="#saveNewCoordinates" prefHeight="110.0"
            prefWidth="110.0" style="-fx-background-color:#a8dca3;" text="Save"/>
    <Button layoutX="525.0" layoutY="645.0" mnemonicParsing="false" onAction="#showInformation" prefHeight="110.0"
            prefWidth="110.0" style="-fx-background-color:#a8dca3;" text="Info"/>


</AnchorPane>
